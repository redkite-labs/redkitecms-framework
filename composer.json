{
  "name": "redkite-labs/redkitecms-framework",
  "description": "RedKiteCms is an Open Source Content Management System Application, built on top of Symfony2 components and TwitterBootstrap frameworks, providing an easy an intuitive user-interface, to improve the website's content management experience",
  "keywords": ["redkite", "redkite labs", "redkite cms", "cms"],
  "homepage": "http://redkite-labs.com",
  "type": "library",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "RedKite Labs",
      "email": "info@redkite-labs.com"
    }
  ],
  "require": {
    "php": ">=5.3.3"
  },
  "require-dev": {
    "php": ">=5.3.3",
    "silex/silex": "~1.2",
    "twig/twig": ">=1.8,<2.0-dev",
    "symfony/security": "~2.6",
    "jms/serializer": "*",
    "symfony/form": "~2.6",
    "symfony/validator": "~2.6",
    "symfony/config": "~2.6",
    "symfony/translation": "~2.6",
    "symfony/locale": "~2.6",
    "symfony/security-csrf": "~2.6",
    "symfony/twig-bridge": "~2.6",
    "symfony/yaml": "~2.6",
    "symfony/finder": "~2.6",
    "symfony/filesystem": "~2.6",
    "symfony/browser-kit": "~2.6",
    "symfony/css-selector": "~2.6",
    "symfony/monolog-bridge": "~2.6",
    "symfony/process": "~2.6",
    "kriswallsmith/assetic": "~1.2",
    "mikey179/vfsStream": "1.1.0",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-0": {
        "RedKiteCms": ["framework", "plugins", "tests/framework"],
        "Controller": "controllers"
    }
  }
}
